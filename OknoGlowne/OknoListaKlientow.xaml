        <Button Name="buttonSortujNazwiskami"  Content="Sortuj nazwiskami" HorizontalAlignment="Left" Margin="662,292,0,0" VerticalAlignment="Top" Width="120" Height="41"/>
        <Button Name="buttonSortujMiastami" Content="Sortuj miastami" HorizontalAlignment="Left" Margin="662,346,0,0" VerticalAlignment="Top" Width="120" Height="41"/>

    </Grid>
